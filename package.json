  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --quiet 'src/**'",
    "lint:fix": "eslint --fix 'src/**'",
    "e2e-local:open": "cypress open --config-file cypress.local.config.ts",
    "e2e-local:run": "cypress run --config-file cypress.local.config.ts",
    "e2e-staging:open": "cypress open --config-file cypress.staging.config.ts",
    "e2e-staging:run": "cypress run --config-file cypress.staging.config.ts",
    "format": "prettier --write '{src,test,graphql}/**/*.{js,jsx,ts,tsx,graphql}'",
    "generate-client": "openapi-generator-cli generate -g typescript-axios -i swagger.yaml -o src/gen --api-package=api --model-package=model --additional-properties=withSeparateModelsAndApi=true",
    "start-mock-server": "prism mock -d swagger.yaml -p 4010",
    "generate-graphql-code": "graphql-codegen --watch",
    "start-graphql-mock-server": "graphql-codegen && sleep 5s && graphql-faker --open #lms-apiのサーバが立ち上がっている時に使用可能",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
